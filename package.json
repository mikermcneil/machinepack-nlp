{
  "name": "machinepack-nlp",
  "version": "0.4.0",
  "description": "Natural language processing (NLP) in Node.js.",
  "scripts": {
    "test": "node ./node_modules/test-machinepack-mocha/bin/testmachinepack-mocha.js"
  },
  "keywords": [
    "Natural Language",
    "machines",
    "machinepack"
  ],
  "author": "mikermcneil",
  "license": "MIT",
  "dependencies": {
    "machine": "^10.3.1",
    "natural": "^0.2.1"
  },
  "devDependencies": {
    "test-machinepack-mocha": "^2.1.1"
  },
  "machinepack": {
    "friendlyName": "Natural Language",
    "extendedDescription": "This machinepack takes advantage of the excellent `natural` package on NPM by Chris Umbel, Rob Ellis, and Russell Mull.",
    "machineDir": "machines/",
    "machines": [
      "levenshtein",
      "tokenize",
      "pluralize",
      "singularize"
    ],
    "testsUrl": "https://travis-ci.org/"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:treelinehq/machinepack-nlp.git"
  }
}
